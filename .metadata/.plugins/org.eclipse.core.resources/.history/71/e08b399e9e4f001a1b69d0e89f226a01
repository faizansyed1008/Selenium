package ArrayClass;

public class InterviewMinnumber {

	public static void main(String[] args) {

		int min = 0;
		int abc[][] = { { 2, 3, 4, 5 }, { 3, 4, 7 }, { 1, 2, 9 } };

		for (int i = 0; i < 3; i++) {

			for (int j = 0; j < 3; j++) {

				{

					if (abc[i][j] < min) {
						min = abc[i][j];
					}
				}

			}
			System.out.println(min);
		}
		
	}
}